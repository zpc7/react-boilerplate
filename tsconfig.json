{
  "compilerOptions": {
    "outDir": "./dist/",
    "alwaysStrict": true,
    "pretty": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "es5",
    "lib": [
      "DOM",
      "ES5",
      "ScriptHost",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise"
    ],
    "module": "esnext",
    "noEmit": false,
    "jsx": "react",
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}